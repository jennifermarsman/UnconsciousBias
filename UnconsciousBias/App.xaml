<Application
    x:Class="UnconsciousBias.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:UnconsciousBias"
    RequestedTheme="Light">

    <Application.Resources>
        <!-- Add your client id here. -->
        <x:String x:Key="ida:ClientID">xxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx</x:String>
        <x:String x:Key="ida:AADInstance">https://login.microsoftonline.com/</x:String>
        <!-- Add your developer tenant domain here. -->
        <x:String x:Key="ida:Domain">common</x:String>
    </Application.Resources>

</Application>
